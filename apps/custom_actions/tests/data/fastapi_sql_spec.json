{
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/execute_sql_query": {
      "post": {
        "summary": "Execute Sql Query",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {}
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "operationId": "execute_sql_query_execute_sql_query_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SQLQuery"
              }
            }
          },
          "required": true
        }
      }
    },
    "/fetch_column_names_and_types": {
      "post": {
        "summary": "Fetch Column Names And Types",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {}
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "operationId": "fetch_column_names_and_types_fetch_column_names_and_types_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TableNames"
              }
            }
          },
          "required": true
        }
      }
    }
  },
  "openapi": "3.1.0",
  "components": {
    "schemas": {
      "SQLQuery": {
        "type": "object",
        "title": "SQLQuery",
        "required": [
          "query"
        ],
        "properties": {
          "query": {
            "type": "string",
            "title": "A SQL query written in standard ANSI SQL syntax, avoiding dialect-specific features like square brackets or proprietary functions."
          },
          "force_csv": {
            "anyOf": [
              {
                "type": "boolean"
              }
            ],
            "title": "Force Csv",
            "default": false
          },
          "force_json": {
            "anyOf": [
              {
                "type": "boolean"
              }
            ],
            "title": "Force Json",
            "default": false
          }
        }
      },
      "TableNames": {
        "type": "object",
        "title": "TableNames",
        "required": [
          "tables"
        ],
        "properties": {
          "tables": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "title": "Tables"
          }
        }
      },
      "ValidationError": {
        "type": "object",
        "title": "ValidationError",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "properties": {
          "loc": {
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        }
      },
      "HTTPValidationError": {
        "type": "object",
        "title": "HTTPValidationError",
        "properties": {
          "detail": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail"
          }
        }
      }
    },
    "securitySchemes": {
      "HTTPBearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}
